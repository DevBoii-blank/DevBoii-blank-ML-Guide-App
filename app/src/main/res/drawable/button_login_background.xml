<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="0.5dp"
        android:right="0.5dp"
        android:bottom="0.5dp"
        android:top="0.5dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#112437" />
            <corners android:radius="0.90dp" />
        </shape>
    </item>
    <item
        android:top="1.25dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="0.5dp" />
            <gradient
                android:angle="-90"

                android:gradientRadius="50%"
                android:startColor="#A37F5B"
                android:endColor="#DCA16A" />

            <stroke android:width="1dp" android:color="#BD8F66" android:radius="0.5dp"/>
        </shape>
    </item>
</layer-list>